{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PathBase": "/api-auth",
  "Keycloak": {
    "realm": "wilder",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "external",
    "resource": "wasm",
    "public-client": true,
    "verify-token-audience": true,
    "use-resource-role-mappings": true,
    "confidential-port": 0
  }
}